{
  "name": "node-module-example-of-thsi",
  "version": "1.0.0",
  "description": "Example of node module",
  "main": "index.js",
  "scripts": {
    "build": "npm run clean && npm run compile",
    "clean": "rimraf lib",
    "compile": "tsc",
    "test": "jest",
    "commit": "npx git-cz"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EricThsi/Node-Module-Example.git"
  },
  "keywords": [
    "Example",
    "Node",
    "Module",
    "TypeScript"
  ],
  "author": "EricThsi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/EricThsi/Node-Module-Example/issues"
  },
  "homepage": "https://github.com/EricThsi/Node-Module-Example#readme",
  "dependencies": {
    "typescript": "^4.0.3"
  },
  "devDependencies": {
    "@types/jest": "^26.0.14",
    "@types/node": "^14.11.8",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^6.8.0",
    "husky": "^4.3.0",
    "jest": "^26.5.2",
    "lint-staged": "^10.4.0",
    "prettier": "^2.1.2",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.4.1",
    "ts-node": "^9.0.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
